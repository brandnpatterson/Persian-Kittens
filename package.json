{
  "name": "zinas",
  "version": "2.0.0",
  "scripts": {
    "autoprefixer": "postcss -u autoprefixer -r style.css",
    "build:all": "npm run build:pug & npm run build:css",
    "build:css": "npm run sass && npm run autoprefixer",
    "build:images": "imagemin app/images/* -o dist/images",
    "build:js": "webpack",
    "build:pug": "pug app/pug/index.pug -o ./",
    "clean": "del index.html style.css app/assets/views dist/index.min.js dist/index.min.js.map dist/views/*.html",
    "clean:images": "del dist/images/*",
    "lint": "eslint app/js",
    "restart": "npm run watch:all",
    "sass": "node-sass --output-style compressed -o ./ app/sass",
    "server": "browser-sync start --server --files='index.html, style.css, dist/index.min.js' --no-notify --port 8888",
    "start": "npm run clean && npm run build:js && npm run build:watch",
    "build:watch": "npm run build:all & npm run watch:all",
    "watch:all": "npm-run-all -p watch:css watch:js watch:pug server",
    "watch:css": "onchange 'app/sass/**/*.sass' -- npm run build:css",
    "watch:js": "onchange 'app/js/**/*.js' -- webpack",
    "watch:pug": "onchange 'app/pug/*.pug' -- npm run build:pug"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
    }
  ],
  "author": "Brandon Patterson",
  "description": "Brandon Patterson's web development portfolio",
  "main": "index.html",
  "license": "ISC",
  "url": "http://www.brandnpatterson.net/zinas",
  "homepage": "http://www.brandnpatterson.net/zinas",
  "dependencies": {
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "browser-sync": "^2.17.5",
    "del": "^2.2.2",
    "eslint": "^2.10.2",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.4.1",
    "imagemin-cli": "^3.0.0",
    "node-sass": "^3.11.2",
    "npm-run-all": "^2.1.1",
    "onchange": "^2.4.0",
    "postcss-cli": "^2.5.2",
    "pug-cli": "^1.0.0-alpha6",
    "uglify-js": "^2.7.4",
    "webpack": "^1.13.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandnpatterson/zinas.git"
  },
  "bugs": {
    "url": "https://github.com/brandnpatterson/brandnpatterson.github.io/zinas/issues"
  }
}
